@html.extend('shared/_layout', function(model){

    @html.block('content', function(model){

        @{
            model.title = "Upload";
        }
        
        <h3>Upload Page</h3>

        <form method="POST" enctype="multipart/form-data" action="/api/upload">
            <input name="files" type="file" multiple>
            <input type="submit">
        </form>

    })

})


